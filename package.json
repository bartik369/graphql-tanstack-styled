{
    "name": "graphql-tanstack-styled",
    "homepage": "https://bartik369.github.com/graphql-tanstack-styled/",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "dev:server": "NODE_OPTIONS='--loader ts-node/esm' node --no-warnings server/server.ts",
        "codegen": "graphql-codegen --config codegen.ts",
        "codegen:watch": "graphql-codegen --config codegen.ts --watch",
        "predeploy": "yarn build",
        "deploy": "gh-pages -d dist"
    },
    "dependencies": {
        "@ant-design/v5-patch-for-react-19": "^1.0.3",
        "@apollo/server": "^4.12.2",
        "@graphql-tools/mock": "^9.0.23",
        "@tanstack/react-query": "^5.81.5",
        "antd": "^5.26.4",
        "express": "^5.1.0",
        "express-graphql": "^0.12.0",
        "graphql": "^16.11.0",
        "graphql-request": "^7.2.0",
        "graphql-tag": "^2.12.6",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router-dom": "^7.6.3",
        "react-toastify": "^11.0.5",
        "styled-components": "^6.1.19",
        "uuidv4": "^6.2.13"
    },
    "devDependencies": {
        "@eslint/js": "^9.29.0",
        "@graphql-codegen/cli": "^5.0.7",
        "@graphql-codegen/typescript": "^4.1.6",
        "@graphql-codegen/typescript-operations": "^4.6.1",
        "@graphql-codegen/typescript-react-query": "^6.1.1",
        "@types/node": "^24.0.10",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@types/styled-components": "^5.1.34",
        "@vitejs/plugin-react": "^4.6.0",
        "eslint": "^9.29.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "gh-pages": "^6.3.0",
        "globals": "^16.2.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.34.1",
        "vite": "^7.0.2"
    }
}